            <div class="container">
                <div class="section-title">
                    <h2>Contact Options</h2>
                    <div class="bar"></div>
                    
                </div>

                <div class="row">
                    <div class="col-lg-5 col-md-12">
                        <div class="contact-info">
                            <ul>
                                <li>
                                    <div class="icon">
                                        <i class="fas fa-map-marker-alt"></i>
                                    </div>
                                    <span>Address</span>
                                    155 Kings Division St, Toronto, ON, M9V 4C1, Canada
                                </li>

                                <li>
                                    <div class="icon">
                                        <i class="fas fa-envelope"></i>
